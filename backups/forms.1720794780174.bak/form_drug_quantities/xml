      <bind nodeset="/drug_quantities/chw_age_in_months" type="string" calculate="difference-in-months(  /drug_quantities/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/drug_quantities/chw_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /drug_quantities/inputs/contact/date_of_birth ))"/>
      <bind nodeset="/drug_quantities/chw_age_display" type="string" calculate="concat(concat( /drug_quantities/chw_age_in_years , if( /drug_quantities/chw_age_in_years =1,' year and ',' years and ')),concat( /drug_quantities/chw_age_in_months  mod 12, if(( /drug_quantities/chw_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/drug_quantities/chw_age_display_fr" type="string" calculate="concat(concat( /drug_quantities/chw_age_in_years , if( /drug_quantities/chw_age_in_years =1,' an et ',' ans et ')),concat( /drug_quantities/chw_age_in_months  mod 12, if(( /drug_quantities/chw_age_in_months  mod 12)=1,' mois',' mois')))"/>
