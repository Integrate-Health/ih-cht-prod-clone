      <bind nodeset="/drug_quantities/chw_sex_fr" type="string" calculate="if( /drug_quantities/chw_sex  = 'male' or  /drug_quantities/chw_sex  = 'homme' or  /drug_quantities/chw_sex  = 'Homme', 'Homme', if( /drug_quantities/chw_sex  = 'female' or  /drug_quantities/chw_sex  = 'femme' or  /drug_quantities/chw_sex  = 'Femme', 'Femme', ''))"/>
