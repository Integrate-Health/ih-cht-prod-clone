      <instance id="contact-summary"/>
      <bind nodeset="/pcime_c_asc/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/pcime_c_asc/inputs/source" type="string"/>
      <bind nodeset="/pcime_c_asc/inputs/source_id" type="string"/>
      <bind nodeset="/pcime_c_asc/inputs/contact/_id" type="string" required="true()"/>
      <bind nodeset="/pcime_c_asc/inputs/contact/name" type="string"/>
      <bind nodeset="/pcime_c_asc/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/pcime_c_asc/inputs/contact/sex" type="string"/>
      <bind nodeset="/pcime_c_asc/inputs/contact/parent" type="string"/>
      <bind nodeset="/pcime_c_asc/patient_age_in_years" type="string" calculate="if (  /pcime_c_asc/inputs/contact/date_of_birth ='', '', floor( difference-in-months(  /pcime_c_asc/inputs/contact/date_of_birth , today() ) div 12 ) )"/>
      <bind nodeset="/pcime_c_asc/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pcime_c_asc/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pcime_c_asc/patient_age_in_months" type="string" calculate="difference-in-months(  /pcime_c_asc/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/pcime_c_asc/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /pcime_c_asc/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/pcime_c_asc/patient_age_display_fr" type="string" calculate="concat(concat( /pcime_c_asc/patient_age_in_years , if( /pcime_c_asc/patient_age_in_years =1,' an et ',' ans et ')),concat( /pcime_c_asc/patient_age_in_months  mod 12, if(( /pcime_c_asc/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
      <bind nodeset="/pcime_c_asc/patient_age_display" type="string" calculate="concat(concat( /pcime_c_asc/patient_age_in_years , if( /pcime_c_asc/patient_age_in_years =1,' year and ',' years and ')),concat( /pcime_c_asc/patient_age_in_months  mod 12, if(( /pcime_c_asc/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/pcime_c_asc/patient_sex_en" type="string" calculate="if( /pcime_c_asc/inputs/contact/sex  = 'male' or  /pcime_c_asc/inputs/contact/sex  = 'homme' or  /pcime_c_asc/inputs/contact/sex  = 'Homme', 'Male', if( /pcime_c_asc/inputs/contact/sex  = 'female' or  /pcime_c_asc/inputs/contact/sex  = 'femme' or  /pcime_c_asc/inputs/contact/sex  = 'Femme', 'Female', ''))"/>
      <bind nodeset="/pcime_c_asc/patient_sex_fr" type="string" calculate="if( /pcime_c_asc/inputs/contact/sex  = 'male' or  /pcime_c_asc/inputs/contact/sex  = 'homme' or  /pcime_c_asc/inputs/contact/sex  = 'Homme', 'Homme', if( /pcime_c_asc/inputs/contact/sex  = 'female' or  /pcime_c_asc/inputs/contact/sex  = 'femme' or  /pcime_c_asc/inputs/contact/sex  = 'Femme', 'Femme', ''))"/>
      <bind nodeset="/pcime_c_asc/visited_contact_uuid" type="string" calculate="../inputs/contact/parent" required="true()"/>
      <bind nodeset="/pcime_c_asc/referral_danger_signs" type="string" calculate="if( /pcime_c_asc/s_danger_signs/has_danger_sign  = 'true' , 'true', 'false')"/>
      <bind nodeset="/pcime_c_asc/referral_MAS_complicated" type="string" calculate="if(( /pcime_c_asc/s_constant/s_constant_child_brachial_perimeter  != '' and  /pcime_c_asc/s_constant/s_constant_child_brachial_perimeter  &lt; 115) and ( /pcime_c_asc/s_nutritional_state/s_nutritional_state_child_has_other_problem  = 'yes' or ( /pcime_c_asc/has_fever  = 'true' or  /pcime_c_asc/has_diarrhea  = 'true' or  /pcime_c_asc/has_cough_cold  = 'true' or  /pcime_c_asc/has_pneumonia  = 'true' or  /pcime_c_asc/has_simple_malaria  = 'true')), 'true', 'false')"/>
      <bind nodeset="/pcime_c_asc/MAS_uncomplicated" type="string" calculate="if(( /pcime_c_asc/s_constant/s_constant_child_brachial_perimeter  != '' and  /pcime_c_asc/s_constant/s_constant_child_brachial_perimeter  &lt; 115) and ( /pcime_c_asc/s_nutritional_state/s_nutritional_state_child_has_other_problem  = 'no' and ( /pcime_c_asc/has_fever  = 'false' and  /pcime_c_asc/has_cough_cold  = 'false' and  /pcime_c_asc/has_diarrhea  = 'false' and  /pcime_c_asc/has_pneumonia  = 'false' and  /pcime_c_asc/has_simple_malaria  = 'false')), 'true', 'false')"/>
