      <bind nodeset="/usp_pcime_followup/inputs/source_id" type="string"/>
      <bind nodeset="/usp_pcime_followup/inputs/contact/_id" type="string" required="true()"/>
      <bind nodeset="/usp_pcime_followup/inputs/contact/name" type="string"/>
      <bind nodeset="/usp_pcime_followup/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/usp_pcime_followup/inputs/contact/sex" type="string"/>
      <bind nodeset="/usp_pcime_followup/inputs/contact/parent" type="string"/>
      <bind nodeset="/usp_pcime_followup/patient_age_in_years" type="string" calculate="if (  /usp_pcime_followup/inputs/contact/date_of_birth ='', '', floor( difference-in-months(  /usp_pcime_followup/inputs/contact/date_of_birth , today() ) div 12 ) )"/>
      <bind nodeset="/usp_pcime_followup/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/usp_pcime_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/usp_pcime_followup/patient_age_in_months" type="string" calculate="difference-in-months(  /usp_pcime_followup/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/usp_pcime_followup/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /usp_pcime_followup/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/usp_pcime_followup/patient_age_display_fr" type="string" calculate="concat(concat( /usp_pcime_followup/patient_age_in_years , if( /usp_pcime_followup/patient_age_in_years =1,' an et ',' ans et ')),concat( /usp_pcime_followup/patient_age_in_months  mod 12, if(( /usp_pcime_followup/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
