      <instance id="contact-summary"/>
      <bind nodeset="/create_new_family/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/create_new_family/inputs/source" type="string"/>
      <bind nodeset="/create_new_family/inputs/source_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/user/contact_id" type="db:person"/>
      <bind nodeset="/create_new_family/inputs/user/fullname" type="string"/>
      <bind nodeset="/create_new_family/inputs/user/name" type="string"/>
      <bind nodeset="/create_new_family/inputs/user/phone" type="string"/>
      <bind nodeset="/create_new_family/inputs/user/roles" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/_id" type="db:clinic"/>
      <bind nodeset="/create_new_family/inputs/contact/name" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/external_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/code" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/parent/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/parent/parent/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/contact/_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/contact/name" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/contact/external_id" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/contact/phone" type="string"/>
      <bind nodeset="/create_new_family/inputs/contact/contact/role" type="string"/>
      <bind nodeset="/create_new_family/creation_number" type="string" calculate="instance('contact-summary')/context/creation_number"/>
      <bind nodeset="/create_new_family/is_creation_mode" type="string" calculate="if( /create_new_family/creation_number  != '' and int( /create_new_family/creation_number ) = 10,'true','false')"/>
      <bind nodeset="/create_new_family/access_not" type="select1" constraint=".='yesForAccess'" required="true()" jr:constraintMsg="jr:itext('/create_new_family/access_not:jr:constraintMsg')" relevant=" /create_new_family/is_creation_mode  = 'false'"/>
      <bind nodeset="/create_new_family/hide_report" type="string" calculate="&quot;true&quot;"/>
      <bind nodeset="/create_new_family/previous_family_code_number" type="string" calculate="int(coalesce(instance('contact-summary')/context/last_family_code,0))"/>
      <bind nodeset="/create_new_family/family_code_number" type="string" calculate="int(coalesce(if( /create_new_family/previous_family_code_number  &lt; 0 or  /create_new_family/previous_family_code_number  = '',0, /create_new_family/previous_family_code_number ),0)) + 1"/>
      <bind nodeset="/create_new_family/family_external_id_generated" type="string" calculate="if( /create_new_family/family_code_number  &lt; 10,concat('0', /create_new_family/family_code_number ), /create_new_family/family_code_number )"/>
      <bind nodeset="/create_new_family/family_code_generated" type="string" calculate="concat( /create_new_family/user_external_id , /create_new_family/family_external_id_generated )"/>
      <bind nodeset="/create_new_family/c_has_birth_certificate" type="string" calculate="if( /create_new_family/add_new_household/register_patient/has_birth_certificate  = 'yes','true','false')"/>
      <bind nodeset="/create_new_family/information/i_household_external_id" type="string" calculate=" /create_new_family/family_external_id_generated " readonly="true()" required="true()"/>
      <bind nodeset="/create_new_family/information/i_user_fullname" type="string" calculate=" /create_new_family/user_fullname " readonly="true()" required="true()"/>
      <bind nodeset="/create_new_family/information/i_user_external_id" type="string" calculate=" /create_new_family/user_external_id " readonly="true()" required="true()"/>
      <bind nodeset="/create_new_family/information/c_household_has_working_latrine" type="select1" required="true()"/>
      <bind nodeset="/create_new_family/information/c_household_has_good_water_access" type="select1" required="true()"/>
      <bind nodeset="/create_new_family/add_new_household/external_id" type="string" calculate="concat( /create_new_family/user_external_id , /create_new_family/family_external_id_generated ,'01')" readonly="true()" required="true()"/>
      <bind nodeset="/create_new_family/add_new_household/type" type="string" calculate="&quot;person&quot;"/>
      <bind nodeset="/create_new_family/add_new_household/name" type="string" required="true()"/>
      <bind nodeset="/create_new_family/add_new_household/selected_role" type="string" calculate="&quot;patient&quot;"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/is_household" type="string" calculate="&quot;true&quot;"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/is_patient" type="string" calculate="&quot;true&quot;"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/selected_sex" type="select1" required="true()"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/dob_method" type="select1" required="true()"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/dob_calendar" type="date" constraint="../age_by_date &gt;=18 and ../age_by_date &lt; 150" required="true()" jr:constraintMsg="jr:itext('/create_new_family/add_new_household/register_patient/dob_calendar:jr:constraintMsg')" relevant="selected(../dob_method,'calendar')"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/age_by_date" type="int" calculate="floor(difference-in-months(../dob_calendar, today()) div 12 )"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/age_years" type="int" constraint=". &gt;= 18 and .&lt;150" required="true()" jr:constraintMsg="jr:itext('/create_new_family/add_new_household/register_patient/age_years:jr:constraintMsg')" relevant="selected(../dob_method,'approx')"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/selected_date_of_birth" type="string" calculate="if(../dob_calendar='' and ../age_years='','',format-date-time(if(not(selected( ../dob_method,'approx')), ../dob_calendar, format-date-time( decimal-date-time( today() ) - (365.25 * ../age_years) - 0 - 0, &quot;%Y-%m-%d&quot; ) ),&quot;%Y-%m-%d&quot;))"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/patient_code" type="string" calculate="../../external_id"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/patient_has_phone" type="select1" required="true()"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/phone" type="int" constraint="regex(., '^\d{9}$')" required="true()" jr:constraintMsg="jr:itext('/create_new_family/add_new_household/register_patient/phone:jr:constraintMsg')" relevant="../patient_has_phone = 'yes'"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/phone_other" type="int" constraint="regex(., '^\d{9}$')" calculate="coalesce(., /create_new_family/user_phone )" jr:constraintMsg="jr:itext('/create_new_family/add_new_household/register_patient/phone_other:jr:constraintMsg')" relevant="../patient_has_phone = 'no'"/>
      <bind nodeset="/create_new_family/add_new_household/register_patient/has_birth_certificate" type="select1" required="true()"/>
