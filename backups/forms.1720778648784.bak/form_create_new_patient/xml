      <bind nodeset="/create_new_patient/inputs/contact/parent/contact/_id" type="string"/>
      <bind nodeset="/create_new_patient/inputs/contact/parent/contact/name" type="string"/>
      <bind nodeset="/create_new_patient/inputs/contact/parent/contact/external_id" type="string"/>
      <bind nodeset="/create_new_patient/inputs/contact/parent/contact/phone" type="string"/>
      <bind nodeset="/create_new_patient/inputs/contact/parent/contact/role" type="string"/>
      <bind nodeset="/create_new_patient/creation_number" type="string" calculate="instance('contact-summary')/context/creation_number"/>
      <bind nodeset="/create_new_patient/parent_phone" type="string" calculate="instance('contact-summary')/context/parent_phone"/>
      <bind nodeset="/create_new_patient/is_creation_mode" type="string" calculate="if( /create_new_patient/creation_number  != '' and int( /create_new_patient/creation_number ) = 10,'true','false')"/>
      <bind nodeset="/create_new_patient/access_not" type="select1" jr:constraintMsg="jr:itext('/create_new_patient/access_not:jr:constraintMsg')" required="true()" constraint=".='yesForAccess'" relevant=" /create_new_patient/is_creation_mode  = 'false'"/>
      <bind nodeset="/create_new_patient/hide_report" type="string" calculate="&quot;true&quot;"/>
      <bind nodeset="/create_new_patient/previous_patient_code_number" type="string" calculate="instance('contact-summary')/context/last_patient_code"/>
      <bind nodeset="/create_new_patient/patient_code_number" type="string" calculate="int(if( /create_new_patient/previous_patient_code_number  = '' or  /create_new_patient/previous_patient_code_number  &lt;= 1,1, /create_new_patient/previous_patient_code_number )) + 1"/>
      <bind nodeset="/create_new_patient/patient_code_generated" type="string" calculate="if( /create_new_patient/patient_code_number  &lt; 10,concat('0', /create_new_patient/patient_code_number ), /create_new_patient/patient_code_number )"/>
      <bind nodeset="/create_new_patient/user_id" type="string" calculate="../inputs/user/contact_id"/>
      <bind nodeset="/create_new_patient/user_uid" type="string" calculate="../inputs/user/contact_id"/>
      <bind nodeset="/create_new_patient/user_name" type="string" calculate="../inputs/user/name"/>
      <bind nodeset="/create_new_patient/user_fullname" type="string" calculate="../inputs/contact/parent/contact/name"/>
      <bind nodeset="/create_new_patient/user_phone" type="string" calculate="coalesce(../inputs/user/phone,../inputs/contact/parent/contact/phone)"/>
      <bind nodeset="/create_new_patient/user_external_id" type="string" calculate="../inputs/contact/parent/contact/external_id"/>
      <bind nodeset="/create_new_patient/user_role" type="string" calculate="../inputs/contact/parent/contact/role"/>
      <bind nodeset="/create_new_patient/user_parent_external_id" type="string" calculate="../inputs/contact/parent/external_id"/>
      <bind nodeset="/create_new_patient/user_parent_code" type="string" calculate="../inputs/contact/parent/code"/>
      <bind nodeset="/create_new_patient/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/create_new_patient/patient_uid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/create_new_patient/patient_external_id" type="string" calculate="../inputs/contact/external_id"/>
      <bind nodeset="/create_new_patient/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/create_new_patient/created_patient_name" type="string" calculate="../add_new_patient/name"/>
      <bind nodeset="/create_new_patient/created_patient_sex" type="string" calculate="../add_new_patient/sex"/>
      <bind nodeset="/create_new_patient/created_patient_date_of_birth" type="string" calculate="../add_new_patient/date_of_birth"/>
      <bind nodeset="/create_new_patient/patient_age_in_years" type="string" calculate="if(../created_patient_date_of_birth = '', '', floor( difference-in-months(../created_patient_date_of_birth, today() ) div 12 ) )"/>
      <bind nodeset="/create_new_patient/patient_age_in_months" type="string" calculate="difference-in-months(../created_patient_date_of_birth, today() )"/>
      <bind nodeset="/create_new_patient/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time(../created_patient_date_of_birth) )"/>
      <bind nodeset="/create_new_patient/household_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/create_new_patient/household_uid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/create_new_patient/household_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/create_new_patient/household_external_id" type="string" calculate="../inputs/contact/external_id"/>
      <bind nodeset="/create_new_patient/household_head_date_of_birth" type="string" calculate="../inputs/contact/contact/date_of_birth"/>
      <bind nodeset="/create_new_patient/household_head_phone" type="string" calculate="../inputs/contact/contact/phone"/>
      <bind nodeset="/create_new_patient/household_head_phone_other" type="string" calculate="../inputs/contact/contact/phone_other"/>
      <bind nodeset="/create_new_patient/village_secteur_id" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/create_new_patient/village_secteur_uid" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/create_new_patient/district_quartier_id" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/district_quartier_uid" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/hospital_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/hospital_uid" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/commune_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/commune_uid" type="string" calculate="../inputs/contact/parent/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/prefecture_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/prefecture_uid" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/region_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/region_uid" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/country_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/country_uid" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/create_new_patient/add_new_patient/external_id" type="string" required="true()" readonly="true()" calculate="concat( /create_new_patient/user_external_id , /create_new_patient/patient_external_id , /create_new_patient/patient_code_generated )"/>
      <bind nodeset="/create_new_patient/add_new_patient/name" type="string" required="true()" constraint="regex(., '.*[A-Za-z].*')"/>
      <bind nodeset="/create_new_patient/add_new_patient/register_patient/is_patient" type="string" calculate="concat('true','')"/>
