      <bind nodeset="/event_register/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/event_register/inputs/source" type="string"/>
      <bind nodeset="/event_register/inputs/source_id" type="string"/>
      <bind nodeset="/event_register/inputs/user/contact_id" type="string"/>
      <bind nodeset="/event_register/inputs/user/name" type="string"/>
      <bind nodeset="/event_register/inputs/user/phone" type="string"/>
      <bind nodeset="/event_register/inputs/contact/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/name" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/inputs/contact/parent/parent/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/event_register/user_id" type="string" calculate="../inputs/user/contact_id"/>
      <bind nodeset="/event_register/user_phone" type="string" calculate="../inputs/user/phone"/>
      <bind nodeset="/event_register/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/event_register/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/event_register/village_secteur_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/event_register/district_quartier_id" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/event_register/hospital_id" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/event_register/commune_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/event_register/prefecture_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/_id"/>
      <bind nodeset="/event_register/region_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/event_register/country_id" type="string" calculate="../inputs/contact/parent/parent/parent/parent/parent/parent/_id"/>
      <bind nodeset="/event_register/event_desease/events" type="select" jr:requiredMsg="jr:itext('/event_register/event_desease/events:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/event_register/event_desease/other_event" type="string" relevant="../events = 'others'"/>
      <bind nodeset="/event_register/event_desease/event_name" type="string" calculate="if(../events = '','',if(../events = 'others',../other_event,jr:choice-name( /event_register/event_desease/events ,' /event_register/event_desease/events ')))"/>
      <bind nodeset="/event_register/event_desease/event_date" type="date" jr:requiredMsg="jr:itext('/event_register/event_desease/event_date:jr:requiredMsg')" constraint=". &lt;= today()" jr:constraintMsg="jr:itext('/event_register/event_desease/event_date:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/event_register/event_desease/village_location_name" type="string" jr:requiredMsg="jr:itext('/event_register/event_desease/village_location_name:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/event_register/event_desease/name_person_in_charge" type="string"/>
      <bind nodeset="/event_register/event_desease/phone_person_in_charge" type="string" constraint="regex(., '^\d{9}$')" jr:constraintMsg="jr:itext('/event_register/event_desease/phone_person_in_charge:jr:constraintMsg')"/>
      <bind nodeset="/event_register/event_desease/health_center_feedback_date" type="date" constraint=". &gt;=  /event_register/event_desease/event_date  and . &lt;= today()" jr:constraintMsg="jr:itext('/event_register/event_desease/health_center_feedback_date:jr:constraintMsg')"/>
      <bind nodeset="/event_register/event_desease/health_center_feedback_location" type="select1" jr:requiredMsg="jr:itext('/event_register/event_desease/health_center_feedback_location:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/event_register/event_desease/health_center_feedback_name" type="string" relevant="../health_center_feedback_location != ''"/>
      <bind nodeset="/event_register/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
