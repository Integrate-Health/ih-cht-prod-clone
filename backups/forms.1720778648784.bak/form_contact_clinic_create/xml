      <bind nodeset="/data/inputs/user/contact_id" type="db:person"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/inputs/user/external_id" type="string"/>
      <bind nodeset="/data/inputs/user/phone" type="string"/>
      <bind nodeset="/data/inputs/contact/_id" type="db:person" calculate="/data/inputs/user/contact_id"/>
      <bind nodeset="/data/inputs/contact/name" type="string" calculate="/data/inputs/contact/name"/>
      <bind nodeset="/data/inputs/contact/role" type="string" calculate="/data/inputs/contact/role"/>
      <bind nodeset="/data/inputs/contact/external_id" type="string" calculate="/data/inputs/contact/external_id"/>
      <bind nodeset="/data/inputs/contact/phone" type="string" calculate="/data/inputs/contact/phone"/>
      <bind nodeset="/data/inputs/contact/parent/_id" type="db:person" calculate="/data/inputs/user/facility_id"/>
      <bind nodeset="/data/inputs/contact/parent/name" type="string" calculate="/data/inputs/contact/parent/name"/>
      <bind nodeset="/data/inputs/contact/parent/external_id" type="string" calculate="/data/inputs/contact/parent/external_id"/>
      <bind nodeset="/data/inputs/contact/parent/code" type="string" calculate="/data/inputs/contact/parent/code"/>
      <bind nodeset="/data/inputs/meta/user_fullname" type="string" calculate="/data/inputs/contact/name" readonly="true()"/>
      <bind nodeset="/data/inputs/meta/user_role" type="string" calculate="/data/inputs/contact/role" readonly="true()"/>
      <bind nodeset="/data/inputs/meta/user_external_id" type="string" calculate="/data/inputs/contact/external_id" readonly="true()"/>
      <bind nodeset="/data/inputs/meta/user_phone" type="string" calculate="coalesce(/data/inputs/user/phone,/data/inputs/contact/phone)"/>
      <bind nodeset="/data/inputs/meta/user_parent_id" type="string" calculate="/data/inputs/contact/parent/_id"/>
      <bind nodeset="/data/inputs/meta/user_parent_name" type="string" calculate="/data/inputs/contact/parent/name"/>
      <bind nodeset="/data/inputs/meta/user_parent_external_id" type="string" calculate="/data/inputs/contact/parent/external_id"/>
      <bind nodeset="/data/inputs/meta/user_parent_code" type="string" calculate="/data/inputs/contact/parent/code"/>
      <bind nodeset="/data/inputs/meta/user_id" type="string" calculate="/data/inputs/user/contact_id"/>
      <bind nodeset="/data/inputs/meta/user_name" type="string" calculate="/data/inputs/user/name"/>
      <bind nodeset="/data/inputs/meta/this_external_id" type="string" calculate="/data/inputs/user/external_id"/>
      <bind nodeset="/data/inputs/meta/family_code_number" type="int" required="true()" constraint="regex(., '^[0-9]+$') and . &gt; 0" jr:constraintMsg="jr:itext('/data/inputs/meta/family_code_number:jr:constraintMsg')"/>
      <bind nodeset="/data/inputs/meta/family_external_id" type="string" calculate="if( /data/inputs/meta/family_code_number  &lt; 10,concat('0', /data/inputs/meta/family_code_number ), /data/inputs/meta/family_code_number )"/>
      <bind nodeset="/data/inputs/meta/c_household_has_working_latrine" type="select1" required="true()"/>
      <bind nodeset="/data/inputs/meta/c_household_has_good_water_access" type="select1" required="true()"/>
      <bind nodeset="/data/contact/external_id" type="string" required="true()" calculate="concat( /data/inputs/meta/user_external_id , /data/inputs/meta/family_external_id ,'01')" readonly="true()"/>
      <bind nodeset="/data/contact/type" type="string"/>
      <bind nodeset="/data/contact/parent" type="string" calculate="."/>
      <bind nodeset="/data/contact/name" type="string" required="true()"/>
      <bind nodeset="/data/contact/selected_role" type="string" calculate="&quot;patient&quot;"/>
      <bind nodeset="/data/contact/register_patient/is_household" type="string" calculate="&quot;true&quot;"/>
      <bind nodeset="/data/contact/register_patient/is_patient" type="string" calculate="&quot;true&quot;"/>
      <bind nodeset="/data/contact/register_patient/selected_sex" type="select1" required="true()"/>
      <bind nodeset="/data/contact/register_patient/dob_method" type="select1" required="true()"/>
      <bind nodeset="/data/contact/register_patient/dob_calendar" type="date" constraint="../age_by_date &gt;=18 and ../age_by_date &lt; 150" relevant="selected(../dob_method,'calendar')" required="true()" jr:constraintMsg="jr:itext('/data/contact/register_patient/dob_calendar:jr:constraintMsg')"/>
      <bind nodeset="/data/contact/register_patient/age_by_date" type="int" calculate="floor(difference-in-months(../dob_calendar, today()) div 12 )"/>
      <bind nodeset="/data/contact/register_patient/age_years" type="int" constraint=". &gt;= 18 and .&lt;150" relevant="selected(../dob_method,'approx')" required="true()" jr:constraintMsg="jr:itext('/data/contact/register_patient/age_years:jr:constraintMsg')"/>
      <bind nodeset="/data/contact/register_patient/selected_date_of_birth" type="string" calculate="if(../dob_calendar='' and ../age_years='','',format-date-time(if(not(selected( ../dob_method,'approx')), ../dob_calendar, format-date-time( decimal-date-time( today() ) - (365.25 * ../age_years) - 0 - 0, &quot;%Y-%m-%d&quot; ) ),&quot;%Y-%m-%d&quot;))"/>
      <bind nodeset="/data/contact/register_patient/patient_code" type="string" required="true()" calculate="../../external_id"/>
      <bind nodeset="/data/contact/register_patient/patient_has_phone" type="select1" required="true()"/>
      <bind nodeset="/data/contact/register_patient/phone" type="int" constraint="regex(., '^\d{9}$')" relevant="../patient_has_phone = 'yes'" required="true()" jr:constraintMsg="jr:itext('/data/contact/register_patient/phone:jr:constraintMsg')"/>
      <bind nodeset="/data/contact/register_patient/phone_other" type="int" constraint="regex(., '^\d{9}$')" calculate="coalesce(., /data/inputs/meta/user_phone )" relevant="../patient_has_phone = 'no'" jr:constraintMsg="jr:itext('/data/contact/register_patient/phone_other:jr:constraintMsg')"/>
      <bind nodeset="/data/contact/register_patient/has_birth_certificate" type="select1" required="true()"/>
      <bind nodeset="/data/contact/register_patient/profession" type="string"/>
      <bind nodeset="/data/contact/role" type="string" calculate="&quot;patient&quot;"/>
      <bind nodeset="/data/contact/sex" type="string" calculate="../register_patient/selected_sex"/>
      <bind nodeset="/data/contact/date_of_birth" type="string" calculate="../register_patient/selected_date_of_birth"/>
      <bind nodeset="/data/contact/phone" type="string" calculate="../register_patient/phone"/>
      <bind nodeset="/data/contact/phone_other" type="string" calculate="../register_patient/phone_other"/>
      <bind nodeset="/data/contact/geolocation" type="string" calculate="concat(../../inputs/meta/location/lat,../../inputs/meta/location/long)"/>
