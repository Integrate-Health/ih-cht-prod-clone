      <bind nodeset="/vaccination_followup/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /vaccination_followup/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/vaccination_followup/patient_age_display_fr" type="string" calculate="concat(concat( /vaccination_followup/patient_age_in_years , if( /vaccination_followup/patient_age_in_years &lt;=1,' an et ',' ans et ')),concat( /vaccination_followup/patient_age_in_months  mod 12, ' mois et '), concat( /vaccination_followup/patient_age_in_days  mod 365, if(( /vaccination_followup/patient_age_in_days  mod 365)&lt;=1,' Jour',' Jours')) )"/>
      <bind nodeset="/vaccination_followup/patient_age_display" type="string" calculate="concat(concat( /vaccination_followup/patient_age_in_years , if( /vaccination_followup/patient_age_in_years =1,' year and ',' years and ')),concat( /vaccination_followup/patient_age_in_months  mod 12, if(( /vaccination_followup/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
