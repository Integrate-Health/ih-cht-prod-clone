      <bind nodeset="/vaccination_followup/visited_contact_uuid" type="string" calculate="../inputs/contact/parent" required="true()"/>
      <bind nodeset="/vaccination_followup/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/vaccination_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
