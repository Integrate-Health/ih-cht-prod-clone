      <instance id="contact-summary"/>
      <bind nodeset="/delivery/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/delivery/inputs/source" type="string"/>
      <bind nodeset="/delivery/inputs/source_id" type="string"/>
      <bind nodeset="/delivery/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/delivery/inputs/contact/name" type="string"/>
      <bind nodeset="/delivery/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/delivery/inputs/contact/sex" type="string"/>
      <bind nodeset="/delivery/inputs/contact/parent" type="string"/>
      <bind nodeset="/delivery/last_preg_uuid" type="string" calculate="instance('contact-summary')/context/lastPregUUID"/>
      <bind nodeset="/delivery/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /delivery/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/delivery/patient_age_in_months" type="string" calculate="difference-in-months(  /delivery/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/delivery/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /delivery/inputs/contact/date_of_birth ))"/>
      <bind nodeset="/delivery/patient_age_display" type="string" calculate="concat(concat( /delivery/patient_age_in_years , if( /delivery/patient_age_in_years =1,' year and ',' years and ')),concat( /delivery/patient_age_in_months  mod 12, if(( /delivery/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/delivery/patient_age_display_fr" type="string" calculate="concat(concat( /delivery/patient_age_in_years , if( /delivery/patient_age_in_years =1,' an et ',' ans et ')),concat( /delivery/patient_age_in_months  mod 12, if(( /delivery/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
      <bind nodeset="/delivery/has_delivered" type="string" calculate="if( /delivery/s_delivery/s_delivery_date  !='', 'true','false')"/>
      <bind nodeset="/delivery/delivery_place" type="string" calculate="jr:choice-name( /delivery/s_delivery/s_delivery_location ,' /delivery/s_delivery/s_delivery_location ')"/>
      <bind nodeset="/delivery/patient_id" type="string" required="true()" calculate="../inputs/contact/_id" jr:requiredMsg="jr:itext('/delivery/patient_id:jr:requiredMsg')"/>
      <bind nodeset="/delivery/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/delivery/patient_sex" type="string" calculate="../inputs/contact/sex"/>
      <bind nodeset="/delivery/patient_sex_en" type="string" calculate="if( /delivery/patient_sex  = 'male' or  /delivery/patient_sex  = 'homme' or  /delivery/patient_sex  = 'Homme', 'Male', if( /delivery/patient_sex  = 'female' or  /delivery/patient_sex  = 'femme' or  /delivery/patient_sex  = 'Femme', 'Female', ''))"/>
      <bind nodeset="/delivery/patient_sex_fr" type="string" calculate="if( /delivery/patient_sex  = 'male' or  /delivery/patient_sex  = 'homme' or  /delivery/patient_sex  = 'Homme', 'Homme', if( /delivery/patient_sex  = 'female' or  /delivery/patient_sex  = 'femme' or  /delivery/patient_sex  = 'Femme', 'Femme', ''))"/>
      <bind nodeset="/delivery/visited_contact_uuid" type="string" required="true()" calculate="../inputs/contact/parent"/>
      <bind nodeset="/delivery/next_visit_display" type="string" calculate="format-date-time(if( /delivery/s_delivery/c_delivery_dif &gt;=7,if( /delivery/s_delivery/s_pnc_today  ='yes',today(), /delivery/s_delivery/s_next_pnc ),date-time(decimal-date-time( /delivery/s_delivery/s_delivery_date  )+ 7)) , &quot;%b %e, %Y&quot;)"/>
      <bind nodeset="/delivery/p_note" readonly="true()" type="string" relevant="0"/>
      <bind nodeset="/delivery/s_child_alive_label_fr" type="string" calculate="if( /delivery/s_delivery/s_babies_delivered  &gt; 1,'Est-ce que tous les nouveaux-nés sont vivants ?','Est-ce que le nouveau-né est vivant ?')"/>
      <bind nodeset="/delivery/s_child_alive_label_en" type="string" calculate="if( /delivery/s_delivery/s_babies_delivered  &gt;1,'Is all childs still alive ?','Is the child still alive ?')"/>
      <bind nodeset="/delivery/mother_has_danger_signs" type="string" calculate="if( /delivery/confirm_danger_signs_mother/non_stop_bleeding  = 'yes' or  /delivery/confirm_danger_signs_mother/persistant_headache  = 'yes' or  /delivery/confirm_danger_signs_mother/stomach_aches  = 'yes' or  /delivery/confirm_danger_signs_mother/danger_sign_fever  = 'yes' or  /delivery/confirm_danger_signs_mother/smelly_or_abnormal_vaginal_discharge  = 'yes' or  /delivery/confirm_danger_signs_mother/breast_abcess  = 'yes' or  /delivery/confirm_danger_signs_mother/blurred_vision  = 'yes' or  /delivery/confirm_danger_signs_mother/calf_pain  = 'yes', 'true', 'false')"/>
      <bind nodeset="/delivery/newborn_has_danger_signs" type="string" calculate="if( /delivery/danger_signs_newborn/refuses_to_eat  = 'yes' or  /delivery/danger_signs_newborn/vomits_everything  = 'yes' or  /delivery/danger_signs_newborn/difficulty_breathing  = 'yes' or  /delivery/danger_signs_newborn/infected_umbilical_cord  = 'yes' or  /delivery/danger_signs_newborn/skin_postules_with_abcess  = 'yes' or  /delivery/danger_signs_newborn/distended_abdomen  = 'yes' or  /delivery/danger_signs_newborn/weak_or_absence_cry  = 'yes' or  /delivery/danger_signs_newborn/unconscious  = 'yes' or  /delivery/danger_signs_newborn/convulsion  = 'yes', 'true', 'false')"/>
      <bind nodeset="/delivery/s_delivery/s_delivery_date" type="date" required="true()" constraint=". &lt;=today()" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_delivery_date:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/c_delivery_dif" type="string" calculate="floor(decimal-date-time(today()) - decimal-date-time( /delivery/s_delivery/s_delivery_date ))"/>
      <bind nodeset="/delivery/s_delivery/s_pnc_today" type="select1" required="true()" relevant=" /delivery/s_delivery/c_delivery_dif &gt;=7" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_pnc_today:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/s_next_pnc" type="date" required="true()" constraint=".&gt;today()" relevant=" /delivery/s_delivery/s_pnc_today  = 'no'" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_next_pnc:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/s_delivery_location" type="select1" required="true()" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_delivery_location:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/s_specify" type="string" required="true()" relevant="selected( /delivery/s_delivery/s_delivery_location , 'other')" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_specify:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/s_skilled_attendance" type="select1" required="true()" relevant="selected( /delivery/s_delivery/s_delivery_location , 'home') or selected( /delivery/s_delivery/s_delivery_location , 'other')" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_skilled_attendance:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/mother_alive" type="select1" required="true()" jr:requiredMsg="jr:itext('/delivery/s_delivery/mother_alive:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/s_babies_delivered" type="int" constraint=".&gt;0 and .&lt;=4" jr:constraintMsg="jr:itext('/delivery/s_delivery/s_babies_delivered:jr:constraintMsg')" required="true()" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_babies_delivered:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/s_child_alive" type="select1" required="true()" relevant=" /delivery/s_delivery/s_babies_delivered &gt; 0" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_child_alive:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/s_child_died_count" type="int" required="true()" relevant=" /delivery/s_delivery/s_child_alive  = 'no' and  /delivery/s_delivery/s_babies_delivered &gt; 1" jr:requiredMsg="jr:itext('/delivery/s_delivery/s_child_died_count:jr:requiredMsg')"/>
      <bind nodeset="/delivery/s_delivery/s_child_died_age" type="select1" required="true()" relevant=" /delivery/s_delivery/s_child_alive  = 'no' and  /delivery/s_delivery/s_babies_delivered &gt; 1"/>
