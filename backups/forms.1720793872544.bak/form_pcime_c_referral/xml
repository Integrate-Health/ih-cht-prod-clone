            <source_id/>
            <t_follow_up_type/>
            <t_last_weight/>
            <t_last_pb/>
            <t_last_temperature/>
            <contact>
              <_id/>
              <name/>
              <date_of_birth>0</date_of_birth>
              <sex/>
              <parent/>
            </contact>
          </inputs>
          <patient_age_in_years>0</patient_age_in_years>
          <patient_age_in_months>0</patient_age_in_months>
          <patient_age_in_days/>
          <patient_age_display/>
          <patient_age_display_fr/>
          <patient_id/>
          <patient_name/>
          <patient_sex/>
