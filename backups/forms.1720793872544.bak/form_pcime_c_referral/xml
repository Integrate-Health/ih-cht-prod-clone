      <instance id="contact-summary"/>
      <bind nodeset="/pcime_c_referral/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/pcime_c_referral/inputs/source" type="string"/>
      <bind nodeset="/pcime_c_referral/inputs/source_id" type="string"/>
      <bind nodeset="/pcime_c_referral/inputs/t_follow_up_type" type="string"/>
      <bind nodeset="/pcime_c_referral/inputs/t_last_weight" type="decimal"/>
      <bind nodeset="/pcime_c_referral/inputs/t_last_pb" type="int"/>
      <bind nodeset="/pcime_c_referral/inputs/t_last_temperature" type="decimal"/>
      <bind nodeset="/pcime_c_referral/inputs/contact/_id" type="db:person" constraint=" /pcime_c_referral/patient_age_in_years  &lt; 5" jr:constraintMsg="jr:itext('/pcime_c_referral/inputs/contact/_id:jr:constraintMsg')"/>
      <bind nodeset="/pcime_c_referral/inputs/contact/name" type="string"/>
      <bind nodeset="/pcime_c_referral/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/pcime_c_referral/inputs/contact/sex" type="string"/>
      <bind nodeset="/pcime_c_referral/inputs/contact/parent" type="string"/>
      <bind nodeset="/pcime_c_referral/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /pcime_c_referral/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/pcime_c_referral/patient_age_in_months" type="string" calculate="difference-in-months(  /pcime_c_referral/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/pcime_c_referral/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /pcime_c_referral/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/pcime_c_referral/patient_age_display" type="string" calculate="concat(concat( /pcime_c_referral/patient_age_in_years , if( /pcime_c_referral/patient_age_in_years =1,' year and ',' years and ')),concat( /pcime_c_referral/patient_age_in_months  mod 12, if(( /pcime_c_referral/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/pcime_c_referral/patient_age_display_fr" type="string" calculate="concat(concat( /pcime_c_referral/patient_age_in_years , if( /pcime_c_referral/patient_age_in_years =1,' an et ',' ans et ')),concat( /pcime_c_referral/patient_age_in_months  mod 12, if(( /pcime_c_referral/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
      <bind nodeset="/pcime_c_referral/patient_id" type="string" required="true()" calculate="../inputs/contact/_id" jr:requiredMsg="jr:itext('/pcime_c_referral/patient_id:jr:requiredMsg')"/>
      <bind nodeset="/pcime_c_referral/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pcime_c_referral/patient_sex" type="string" calculate="../inputs/contact/sex"/>
      <bind nodeset="/pcime_c_referral/patient_sex_en" type="string" calculate="if( /pcime_c_referral/patient_sex  = 'male' or  /pcime_c_referral/patient_sex  = 'homme' or  /pcime_c_referral/patient_sex  = 'Homme', 'Male', if( /pcime_c_referral/patient_sex  = 'female' or  /pcime_c_referral/patient_sex  = 'femme' or  /pcime_c_referral/patient_sex  = 'Femme', 'Female', ''))"/>
      <bind nodeset="/pcime_c_referral/patient_sex_fr" type="string" calculate="if( /pcime_c_referral/patient_sex  = 'male' or  /pcime_c_referral/patient_sex  = 'homme' or  /pcime_c_referral/patient_sex  = 'Homme', 'Homme', if( /pcime_c_referral/patient_sex  = 'female' or  /pcime_c_referral/patient_sex  = 'femme' or  /pcime_c_referral/patient_sex  = 'Femme', 'Femme', ''))"/>
