      <instance>
        <data id="contact:person:create" prefix="J1!contact:person:create!" delimiter="#" version="2024-07-12  10-40">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <type/>
              <name/>
            </user>
            <contact>
              <_id/>
              <role/>
              <username/>
              <name/>
              <extid/>
              <external_id/>
            </contact>
            <meta>
              <location>
                <lat/>
                <long/>
              </location>
            </meta>
          </inputs>
          <person>
            <type>person</type>
            <parent>PARENT</parent>
            <place_contact_select>no</place_contact_select>
            <family_contact_select>no</family_contact_select>
            <place_contact/>
            <family_contact/>
            <name/>
            <dob_method>approx</dob_method>
            <dob_calendar/>
            <age_years/>
            <age_months>0</age_months>
            <date_of_birth/>
            <sex/>
            <code_type/>
            <code_w/>
            <code_x/>
            <code_y/>
            <unique_code/>
            <external_id/>
            <code/>
            <info/>
            <role_x/>
            <role/>
            <phone/>
            <phone_alternate/>
            <pcime_info/>
            <meta>
              <created_by/>
              <created_by_role/>
              <created_by_external_id/>
              <created_by_name/>
              <created_by_place/>
            </meta>
            <geolocation/>
          </person>
          <user_id/>
          <user_name/>
          <user_contact_name/>
          <user_role/>
          <user_external_id/>
          <user_facility_id/>
          <user_latitude/>
          <user_longitude/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/type" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/inputs/contact/_id" type="db:person" readonly="true()" calculate=" /data/user_id "/>
      <bind nodeset="/data/inputs/contact/role" readonly="true()" type="string" calculate=" /data/user_role "/>
      <bind nodeset="/data/inputs/contact/username" readonly="true()" type="string" calculate=" /data/user_name "/>
      <bind nodeset="/data/inputs/contact/name" readonly="true()" type="string" calculate=" /data/user_contact_name "/>
      <bind nodeset="/data/inputs/contact/extid" type="string" calculate=" /data/user_external_id "/>
      <bind nodeset="/data/inputs/contact/external_id" readonly="true()" type="string"/>
      <bind nodeset="/data/inputs/meta/location/lat" type="string"/>
      <bind nodeset="/data/inputs/meta/location/long" type="string"/>
      <bind nodeset="/data/user_id" type="string" calculate="../inputs/user/contact_id"/>
      <bind nodeset="/data/user_name" type="string" calculate="../inputs/user/name"/>
      <bind nodeset="/data/user_contact_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/data/user_role" type="string" calculate="../inputs/contact/role"/>
      <bind nodeset="/data/user_external_id" type="string" calculate="../inputs/contact/external_id"/>
      <bind nodeset="/data/user_facility_id" type="string" calculate="../inputs/user/facility_id"/>
      <bind nodeset="/data/user_latitude" type="string" calculate="../inputs/meta/location/lat"/>
      <bind nodeset="/data/user_longitude" type="string" calculate="../inputs/meta/location/long"/>
      <bind nodeset="/data/person/type" type="string" calculate="../../inputs/user/type"/>
      <bind nodeset="/data/person/parent" type="string" calculate="."/>
      <bind nodeset="/data/person/place_contact_select" type="select1" required="true()" relevant=" /data/user_role  !=&quot;chw&quot;"/>
      <bind nodeset="/data/person/family_contact_select" type="select1" required="true()" relevant=" /data/user_role  !='chw' and  /data/user_role !='chw_supervisor'"/>
      <bind nodeset="/data/person/place_contact" type="string" calculate="coalesce( /data/person/place_contact_select ,'no')"/>
      <bind nodeset="/data/person/family_contact" type="string" calculate="coalesce( /data/person/family_contact_select ,'no')"/>
      <bind nodeset="/data/person/name" type="string" required="true()"/>
      <bind nodeset="/data/person/dob_method" type="select1" required="true()"/>
      <bind nodeset="/data/person/dob_calendar" type="date" jr:constraintMsg="jr:itext('/data/person/dob_calendar:jr:constraintMsg')" constraint=".&lt;today()" required="true()" relevant="selected(../dob_method,'calendar')"/>
      <bind nodeset="/data/person/age_years" type="int" jr:constraintMsg="jr:itext('/data/person/age_years:jr:constraintMsg')" constraint=".&gt;=0 and .&lt;100" required="true()" relevant="selected(../dob_method,'approx')"/>
      <bind nodeset="/data/person/age_months" type="int" jr:constraintMsg="jr:itext('/data/person/age_months:jr:constraintMsg')" constraint=". &gt;= 0 and . &lt;= 11" required="true()" relevant="selected(../dob_method,'approx')"/>
      <bind nodeset="/data/person/date_of_birth" type="string" calculate="format-date-time(if(not(selected( ../dob_method,'approx')), ../dob_calendar, format-date-time( decimal-date-time( today() ) - (365.25 * ../age_years) - (30.5 * ../age_months ) - 0, &quot;%Y-%m-%d&quot; ) ),&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/person/sex" type="select1" required="true()"/>
      <bind nodeset="/data/person/code_type" type="select1" relevant=" /data/user_role  !=&quot;chw&quot;"/>
      <bind nodeset="/data/person/code_w" type="string" jr:constraintMsg="jr:itext('/data/person/code_w:jr:constraintMsg')" constraint="regex(.,'^[A-Z]{6}-\d{9}$')" required="true()" relevant=" /data/user_role !=&quot;chw&quot; and concat(../code_type,'')='yes'"/>
      <bind nodeset="/data/person/code_x" type="string" jr:constraintMsg="jr:itext('/data/person/code_x:jr:constraintMsg')" constraint="regex(.,'^[A-Z]{0}\d{4}$')" required="true()" relevant=" /data/user_role =&quot;chw&quot;"/>
      <bind nodeset="/data/person/code_y" type="string" jr:constraintMsg="jr:itext('/data/person/code_y:jr:constraintMsg')" constraint="regex(.,'^[A-Z]{6}-\d{3}$')" required="true()" relevant=" /data/user_role !=&quot;chw&quot; and concat(../code_type,'')!='yes'"/>
      <bind nodeset="/data/person/unique_code" type="string" calculate="coalesce(coalesce(../code_w,../code_x),../code_y)"/>
      <bind nodeset="/data/person/external_id" type="string" calculate="if( /data/user_role =&quot;chw&quot;,concat( /data/user_external_id ,'00',../unique_code),concat(../unique_code,''))"/>
      <bind nodeset="/data/person/code" type="string" calculate="../external_id"/>
      <bind nodeset="/data/person/info" readonly="true()" type="string"/>
      <bind nodeset="/data/person/role_x" type="select1" calculate="coalesce(../role_x,../role)" required="true()" relevant=" /data/user_role !='chw'"/>
      <bind nodeset="/data/person/role" type="string" calculate="if( /data/user_role ='chw','patient',concat(../role_x,''))" required="true()"/>
      <bind nodeset="/data/person/phone" type="string" constraint="regex(.,'^(\d{2}\s?){4}$')" jr:constraintMsg="jr:itext('/data/person/phone:jr:constraintMsg')"/>
      <bind nodeset="/data/person/phone_alternate" type="string" constraint="regex(.,'^(\d{2}\s?){4}$')" jr:constraintMsg="jr:itext('/data/person/phone_alternate:jr:constraintMsg')"/>
      <bind nodeset="/data/person/pcime_info" readonly="true()" type="string" relevant="floor(difference-in-months(  /data/person/date_of_birth , today() ) div 12 ) &lt; 5"/>
      <bind nodeset="/data/person/meta/created_by" type="string" calculate=" /data/user_id "/>
      <bind nodeset="/data/person/meta/created_by_role" type="string" calculate=" /data/user_role "/>
      <bind nodeset="/data/person/meta/created_by_external_id" type="string" calculate=" /data/user_external_id "/>
      <bind nodeset="/data/person/meta/created_by_name" type="string" calculate=" /data/user_contact_name "/>
      <bind nodeset="/data/person/meta/created_by_place" type="string" calculate=" /data/user_facility_id "/>
      <bind nodeset="/data/person/geolocation" type="string" calculate="concat( /data/user_latitude , /data/user_longitude )"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/data/inputs">
      <label ref="jr:itext('/data/inputs:label')"/>
      <group appearance="hidden" ref="/data/inputs/user">
        <label ref="jr:itext('/data/inputs/user:label')"/>
        <input ref="/data/inputs/user/contact_id">
