      <bind nodeset="/prenatal_followup/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/prenatal_followup/inputs/source" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/source_id" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/t_follow_up_count" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/t_close_out" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/t_week_since_lmp" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/t_delivery_date" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/prenatal_followup/inputs/contact/name" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/contact/sex" type="string"/>
      <bind nodeset="/prenatal_followup/inputs/contact/parent" type="string"/>
      <bind nodeset="/prenatal_followup/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /prenatal_followup/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/prenatal_followup/patient_age_in_months" type="string" calculate="difference-in-months(  /prenatal_followup/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/prenatal_followup/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /prenatal_followup/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/prenatal_followup/patient_age_display" type="string" calculate="concat(concat( /prenatal_followup/patient_age_in_years , if( /prenatal_followup/patient_age_in_years =1,' year and ',' years and ')),concat( /prenatal_followup/patient_age_in_months  mod 12, if(( /prenatal_followup/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/prenatal_followup/patient_age_display_fr" type="string" calculate="concat(concat( /prenatal_followup/patient_age_in_years , if( /prenatal_followup/patient_age_in_years =1,' an et ',' ans et ')),concat( /prenatal_followup/patient_age_in_months  mod 12, if(( /prenatal_followup/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
      <bind nodeset="/prenatal_followup/patient_id" type="string" jr:requiredMsg="jr:itext('/prenatal_followup/patient_id:jr:requiredMsg')" required="true()" calculate="../inputs/contact/_id"/>
      <bind nodeset="/prenatal_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/prenatal_followup/patient_sex" type="string" calculate="../inputs/contact/sex"/>
      <bind nodeset="/prenatal_followup/patient_sex_en" type="string" calculate="if( /prenatal_followup/patient_sex  = 'male' or  /prenatal_followup/patient_sex  = 'homme' or  /prenatal_followup/patient_sex  = 'Homme', 'Male', if( /prenatal_followup/patient_sex  = 'female' or  /prenatal_followup/patient_sex  = 'femme' or  /prenatal_followup/patient_sex  = 'Femme', 'Female', ''))"/>
      <bind nodeset="/prenatal_followup/patient_sex_fr" type="string" calculate="if( /prenatal_followup/patient_sex  = 'male' or  /prenatal_followup/patient_sex  = 'homme' or  /prenatal_followup/patient_sex  = 'Homme', 'Homme', if( /prenatal_followup/patient_sex  = 'female' or  /prenatal_followup/patient_sex  = 'femme' or  /prenatal_followup/patient_sex  = 'Femme', 'Femme', ''))"/>
      <bind nodeset="/prenatal_followup/visited_contact_uuid" type="string" required="true()" calculate="../inputs/contact/parent"/>
      <bind nodeset="/prenatal_followup/delivery_date" type="string" calculate="concat(../inputs/t_delivery_date,'')"/>
      <bind nodeset="/prenatal_followup/display_delivery_date" type="string" calculate="format-date-time( /prenatal_followup/delivery_date , &quot;%Y-%n-%e&quot;)"/>
      <bind nodeset="/prenatal_followup/follow_up_count" type="string" calculate="number(../inputs/t_follow_up_count)"/>
      <bind nodeset="/prenatal_followup/follow_up_count_label_fr" type="string" calculate="if( /prenatal_followup/follow_up_count  = 1, &quot;Suivi 48 Heures&quot;, if( /prenatal_followup/follow_up_count  = 2, &quot;Suivi 5 jours&quot;, if( /prenatal_followup/follow_up_count  = 3, &quot;Suivi 12 jours&quot;, if( /prenatal_followup/follow_up_count  = 4, &quot;Suivi 19 jours&quot;, if( /prenatal_followup/follow_up_count  = 5, &quot;Suivi 26 jours&quot;, if( /prenatal_followup/follow_up_count  = 6, &quot;Suivi 33 jours&quot;, if( /prenatal_followup/follow_up_count  = 7, &quot;Suivi 40 jours&quot;, if( /prenatal_followup/follow_up_count  = 8, &quot;Suivi 48 jours&quot;, &quot;&quot;))))))))"/>
      <bind nodeset="/prenatal_followup/has_danger_signs" type="string" calculate="if( /prenatal_followup/s_ds/danger_signs_edema  = 'yes' or  /prenatal_followup/s_ds/danger_signs_fever  = 'yes' or  /prenatal_followup/s_ds/danger_signs_headaches  = 'yes' or  /prenatal_followup/s_ds/danger_signs_upper_abdominal_pain  = 'yes' or  /prenatal_followup/s_ds/danger_signs_swelling_abdominal_pain  = 'yes' or  /prenatal_followup/s_ds/danger_signs_fetal_movement  = 'yes' or  /prenatal_followup/s_ds/danger_signs_vertigo  = 'yes' or  /prenatal_followup/s_ds/danger_signs_water_losses  = 'yes' or  /prenatal_followup/s_ds/danger_signs_vaginal_bleeding  = 'yes' or  /prenatal_followup/s_ds/danger_signs_vaginal_discharge  = 'yes' or  /prenatal_followup/s_ds/danger_signs_pain_urinating  = 'yes' or  /prenatal_followup/s_ds/danger_signs_extreme_fatigue  = 'yes' or  /prenatal_followup/s_ds/danger_signs_fast_breathing  = 'yes' or  /prenatal_followup/s_ds/danger_signs_pallor_conjunctiva  = 'yes' or  /prenatal_followup/s_ds/danger_signs_stool_color  = 'yes' or  /prenatal_followup/s_ds/danger_signs_cold_skin  = 'yes' or  /prenatal_followup/s_ds/danger_signs_convulsions  = 'yes','true','false')"/>
      <bind nodeset="/prenatal_followup/dif_anc_1_date" type="string" calculate="floor(decimal-date-time( if( /prenatal_followup/follow_up_count &gt;1, /prenatal_followup/s_vad_1/s_anc_next_anc ,coalesce( /prenatal_followup/s_vad_1/s_anc_1_date ,  /prenatal_followup/s_vad_1/s_anc_next_anc ))) - decimal-date-time(today()))"/>
      <bind nodeset="/prenatal_followup/refer_vat_faf" type="string" calculate=" /prenatal_followup/dif_anc_1_date "/>
      <bind nodeset="/prenatal_followup/week_since_lmp" type="string" calculate="number( /prenatal_followup/inputs/t_week_since_lmp )"/>
      <bind nodeset="/prenatal_followup/referral" type="string" calculate=" /prenatal_followup/has_danger_signs  = 'true' or ( /prenatal_followup/s_vad_1/s_anc_1_planned  = 'no' and  /prenatal_followup/s_vad_1/s_anc_1_visit  = 'no') or (( /prenatal_followup/s_vat_faf_analysis/s_vat  = 'no' or  /prenatal_followup/s_vat_faf_analysis/s_faf  = 'no') and ( /prenatal_followup/dif_anc_1_date  &gt; 7))"/>
      <bind nodeset="/prenatal_followup/next_anc_visit_follow_up_date" type="string" calculate="format-date-time(date-time(decimal-date-time( /prenatal_followup/s_vad_1/s_anc_next_anc )+7),&quot;%b %e, %Y&quot;)"/>
