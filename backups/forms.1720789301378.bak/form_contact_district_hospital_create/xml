      <instance>
        <data id="contact:district_hospital:create" prefix="J1!contact:district_hospital:create!" delimiter="#" version="2018-01-24">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <type/>
              <name/>
            </user>
            <contact>
              <_id/>
              <role/>
              <username/>
              <name/>
              <external_id/>
            </contact>
          </inputs>
          <district_hospital>
            <district_external_id/>
            <district_external_name/>
            <external_id/>
            <name/>
            <contact/>
            <notes/>
            <geolocation/>
          </district_hospital>
          <contact>
            <type>person</type>
            <parent>PARENT</parent>
            <place_contact>yes</place_contact>
            <name/>
            <dob_method>approx</dob_method>
            <dob_calendar/>
            <age_years/>
            <age_months>0</age_months>
            <date_of_birth/>
            <sex/>
            <code_type>no</code_type>
            <code_w/>
            <code_x/>
            <code_y/>
            <unique_code/>
            <external_id/>
            <code/>
            <code_z/>
            <info/>
            <role_x/>
            <role/>
            <phone/>
            <phone_alternate/>
            <meta>
              <created_by/>
              <created_by_role/>
              <created_by_external_id/>
              <created_by_name/>
              <created_by_place/>
            </meta>
            <geolocation/>
          </contact>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs/user" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/type" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/inputs/contact/_id" type="db:person" calculate="/data/inputs/user/contact_id" readonly="true()"/>
      <bind nodeset="/data/inputs/contact/role" readonly="true()" type="string" calculate="/data/inputs/contact/role"/>
      <bind nodeset="/data/inputs/contact/username" readonly="true()" type="string" calculate="../../user/name"/>
      <bind nodeset="/data/inputs/contact/name" readonly="true()" type="string" calculate="/data/inputs/contact/name"/>
      <bind nodeset="/data/inputs/contact/external_id" readonly="true()" type="string" calculate="/data/inputs/contact/external_id"/>
      <bind nodeset="/data/district_hospital/district_external_id" type="string" required="true()"/>
      <bind nodeset="/data/district_hospital/district_external_name" type="string" required="true()"/>
      <bind nodeset="/data/district_hospital/external_id" type="string" required="true()"/>
      <bind nodeset="/data/district_hospital/name" type="string" required="true()"/>
      <bind nodeset="/data/district_hospital/contact" type="string" calculate="&quot;NEW&quot;"/>
      <bind nodeset="/data/district_hospital/notes" type="string"/>
      <bind nodeset="/data/district_hospital/geolocation" type="string" calculate="concat(concat('',../../inputs/meta/location/lat), concat('',../../inputs/meta/location/long))"/>
      <bind nodeset="/data/contact/type" type="string"/>
      <bind nodeset="/data/contact/parent" type="string" calculate="."/>
      <bind nodeset="/data/contact/place_contact" type="string" calculate="concat('yes','')"/>
      <bind nodeset="/data/contact/name" type="string" required="true()"/>
      <bind nodeset="/data/contact/dob_method" type="select1"/>
      <bind nodeset="/data/contact/dob_calendar" type="date" jr:constraintMsg="jr:itext('/data/contact/dob_calendar:jr:constraintMsg')" relevant="selected(../dob_method,'calendar')" constraint=".&lt;today()" required="true()"/>
      <bind nodeset="/data/contact/age_years" type="int" jr:constraintMsg="jr:itext('/data/contact/age_years:jr:constraintMsg')" relevant="selected(../dob_method,'approx')" constraint=". &gt;= 0" required="true()"/>
      <bind nodeset="/data/contact/age_months" type="int" jr:constraintMsg="jr:itext('/data/contact/age_months:jr:constraintMsg')" relevant="selected(../dob_method,'approx')" constraint=". &gt;= 0 and . &lt;= 11" required="true()"/>
      <bind nodeset="/data/contact/date_of_birth" type="string" calculate="format-date-time(if(not(selected( ../dob_method,'approx')), ../dob_calendar, format-date-time( decimal-date-time( today() ) - (365.25 * ../age_years) - (30.5 * ../age_months ) - 0, &quot;%Y-%m-%d&quot; ) ),&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/contact/sex" type="select1" required="true()"/>
      <bind nodeset="/data/contact/code_type" type="select1" relevant="concat(../meta/created_by_role,'') !=&quot;chw&quot;" required="true()"/>
      <bind nodeset="/data/contact/code_w" type="string" jr:constraintMsg="jr:itext('/data/contact/code_w:jr:constraintMsg')" relevant="concat(../meta/created_by_role,'')!=&quot;chw&quot; and concat(../code_type,'')='yes'" constraint="regex(.,'^[A-Z]{6}-\d{9}$')" required="true()"/>
      <bind nodeset="/data/contact/code_x" type="string" jr:constraintMsg="jr:itext('/data/contact/code_x:jr:constraintMsg')" relevant="concat(../meta/created_by_role,'')=&quot;chw&quot;" constraint="regex(.,'^[A-Z]{0}\d{4}$')" required="true()"/>
      <bind nodeset="/data/contact/code_y" type="string" jr:constraintMsg="jr:itext('/data/contact/code_y:jr:constraintMsg')" relevant="concat(../meta/created_by_role,'') !=&quot;chw&quot; and concat(../code_type,'')!='yes'" constraint="regex(.,'^[A-Z]{6}-\d{3}$')" required="true()"/>
      <bind nodeset="/data/contact/unique_code" type="string" calculate="coalesce(coalesce(../code_w,../code_x),../code_y)"/>
      <bind nodeset="/data/contact/external_id" type="string" calculate="if(concat(../meta/created_by_role,'')=&quot;chw&quot;,concat(../meta/created_by_external_id,'00',../unique_code),concat(../unique_code,''))"/>
      <bind nodeset="/data/contact/code" type="string" calculate="../external_id"/>
      <bind nodeset="/data/contact/code_z" type="string" calculate="../code"/>
      <bind nodeset="/data/contact/info" readonly="true()" type="string"/>
      <bind nodeset="/data/contact/role_x" type="select1" calculate="coalesce(../role_x,../role)" relevant="concat(../meta/created_by_role,'')!='chw'" required="true()"/>
      <bind nodeset="/data/contact/role" type="string" calculate="if(concat(../meta/created_by_role,'')='chw','patient',concat(../role_x,''))" required="true()"/>
      <bind nodeset="/data/contact/phone" type="string" jr:constraintMsg="jr:itext('/data/contact/phone:jr:constraintMsg')" constraint="regex(.,'^(\d{2}\s?){4}$')"/>
      <bind nodeset="/data/contact/phone_alternate" type="string" jr:constraintMsg="jr:itext('/data/contact/phone_alternate:jr:constraintMsg')" constraint="regex(.,'^(\d{2}\s?){4}$')"/>
      <bind nodeset="/data/contact/meta/created_by" type="string" calculate="/data/inputs/user/contact_id"/>
