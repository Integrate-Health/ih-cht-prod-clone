      <bind nodeset="/women_emergency_followup/inputs/contact/_id" type="db:person" constraint=" /women_emergency_followup/patient_age_in_years  &lt; 5" jr:constraintMsg="jr:itext('/women_emergency_followup/inputs/contact/_id:jr:constraintMsg')"/>
