      <bind nodeset="/women_emergency_followup/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /women_emergency_followup/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/women_emergency_followup/patient_age_in_months" type="string" calculate="difference-in-months(  /women_emergency_followup/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/women_emergency_followup/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /women_emergency_followup/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/women_emergency_followup/patient_age_display" type="string" calculate="concat(concat( /women_emergency_followup/patient_age_in_years , if( /women_emergency_followup/patient_age_in_years =1,' year and ',' years and ')),concat( /women_emergency_followup/patient_age_in_months  mod 12, if(( /women_emergency_followup/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/women_emergency_followup/patient_age_display_fr" type="string" calculate="concat(concat( /women_emergency_followup/patient_age_in_years , if( /women_emergency_followup/patient_age_in_years =1,' an et ',' ans et ')),concat( /women_emergency_followup/patient_age_in_months  mod 12, if(( /women_emergency_followup/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
