      <bind nodeset="/malnutrition_followup/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/malnutrition_followup/inputs/source" type="string"/>
      <bind nodeset="/malnutrition_followup/inputs/source_id" type="string"/>
      <bind nodeset="/malnutrition_followup/inputs/t_follow_up_type" type="string"/>
      <bind nodeset="/malnutrition_followup/inputs/t_follow_up_count" type="string"/>
      <bind nodeset="/malnutrition_followup/inputs/t_num_yellow_strips" type="string"/>
      <bind nodeset="/malnutrition_followup/inputs/t_assessment_date" type="dateTime"/>
      <bind nodeset="/malnutrition_followup/inputs/t_weight_from_assessment" type="decimal"/>
      <bind nodeset="/malnutrition_followup/inputs/t_pb_from_assessment" type="int"/>
      <bind nodeset="/malnutrition_followup/inputs/t_atpe_given_quantity" type="int"/>
      <bind nodeset="/malnutrition_followup/inputs/contact/_id" type="db:person" constraint=" /malnutrition_followup/patient_age_in_years  &lt; 5" jr:constraintMsg="jr:itext('/malnutrition_followup/inputs/contact/_id:jr:constraintMsg')"/>
      <bind nodeset="/malnutrition_followup/inputs/contact/name" type="string"/>
      <bind nodeset="/malnutrition_followup/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/malnutrition_followup/inputs/contact/sex" type="string"/>
      <bind nodeset="/malnutrition_followup/inputs/contact/parent" type="string"/>
      <bind nodeset="/malnutrition_followup/weight_from_assessment" type="string" calculate="../inputs/t_weight_from_assessment"/>
      <bind nodeset="/malnutrition_followup/pb_from_assessment" type="string" calculate="../inputs/t_pb_from_assessment"/>
      <bind nodeset="/malnutrition_followup/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /malnutrition_followup/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/malnutrition_followup/patient_age_in_months" type="string" calculate="difference-in-months(  /malnutrition_followup/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/malnutrition_followup/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /malnutrition_followup/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/malnutrition_followup/patient_age_display" type="string" calculate="concat(concat( /malnutrition_followup/patient_age_in_years , if( /malnutrition_followup/patient_age_in_years =1,' year and ',' years and ')),concat( /malnutrition_followup/patient_age_in_months  mod 12, if(( /malnutrition_followup/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/malnutrition_followup/patient_age_display_fr" type="string" calculate="concat(concat( /malnutrition_followup/patient_age_in_years , if( /malnutrition_followup/patient_age_in_years =1,' an et ',' ans et ')),concat( /malnutrition_followup/patient_age_in_months  mod 12, if(( /malnutrition_followup/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
      <bind nodeset="/malnutrition_followup/patient_id" type="string" required="true()" jr:requiredMsg="jr:itext('/malnutrition_followup/patient_id:jr:requiredMsg')" calculate="../inputs/contact/_id"/>
      <bind nodeset="/malnutrition_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/malnutrition_followup/patient_sex" type="string" calculate="../inputs/contact/sex"/>
      <bind nodeset="/malnutrition_followup/patient_sex_en" type="string" calculate="if( /malnutrition_followup/patient_sex  = 'male' or  /malnutrition_followup/patient_sex  = 'homme' or  /malnutrition_followup/patient_sex  = 'Homme', 'Male', if( /malnutrition_followup/patient_sex  = 'female' or  /malnutrition_followup/patient_sex  = 'femme' or  /malnutrition_followup/patient_sex  = 'Femme', 'Female', ''))"/>
      <bind nodeset="/malnutrition_followup/patient_sex_fr" type="string" calculate="if( /malnutrition_followup/patient_sex  = 'male' or  /malnutrition_followup/patient_sex  = 'homme' or  /malnutrition_followup/patient_sex  = 'Homme', 'Homme', if( /malnutrition_followup/patient_sex  = 'female' or  /malnutrition_followup/patient_sex  = 'femme' or  /malnutrition_followup/patient_sex  = 'Femme', 'Femme', ''))"/>
      <bind nodeset="/malnutrition_followup/visited_contact_uuid" type="string" required="true()" calculate="../inputs/contact/parent"/>
      <bind nodeset="/malnutrition_followup/follow_up_count" type="string" calculate="../inputs/t_follow_up_count"/>
      <bind nodeset="/malnutrition_followup/follow_up_count_number" type="string" calculate="number(../inputs/t_follow_up_count)"/>
      <bind nodeset="/malnutrition_followup/follow_up_count_label_en" type="string" calculate="if( /malnutrition_followup/follow_up_count_number  = 1, &quot;7 days&quot;, if( /malnutrition_followup/follow_up_count_number  = 2, &quot;14 days&quot;, if( /malnutrition_followup/follow_up_count_number  = 3, &quot;21 days&quot;, if( /malnutrition_followup/follow_up_count_number  = 4, &quot;28 days&quot;, if( /malnutrition_followup/follow_up_count_number  = 5, &quot;42 days&quot;, if( /malnutrition_followup/follow_up_count_number  = 6, &quot;56 days&quot;, if( /malnutrition_followup/follow_up_count_number  = 7, &quot;70 days&quot;, if( /malnutrition_followup/follow_up_count_number  = 8, &quot;84 days&quot;, &quot;&quot;))))))))"/>
