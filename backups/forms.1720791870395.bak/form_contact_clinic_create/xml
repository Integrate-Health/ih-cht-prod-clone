      <bind nodeset="/data/contact/age_years" type="int" relevant="../dob_method = 'approx'" constraint=". &gt;= 0" jr:constraintMsg="jr:itext('/data/contact/age_years:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/age_months" type="int" relevant="../dob_method = 'approx'" constraint=". &gt;= 0 and . &lt;= 11" jr:constraintMsg="jr:itext('/data/contact/age_months:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/date_of_birth" type="string" calculate="format-date-time(if(not(selected( ../dob_method,'approx')), ../dob_calendar, format-date-time( decimal-date-time( today() ) - (365.25 * ../age_years) - (30.5 * ../age_months ) - 5, &quot;%Y-%m-%d&quot; ) ),&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/contact/sex" type="select1" required="true()"/>
      <bind nodeset="/data/contact/code_type" type="select1" relevant="../meta/created_by_role !=&quot;chw&quot;" required="true()"/>
      <bind nodeset="/data/contact/code_w" type="string" relevant="../meta/created_by_role != &quot;chw&quot; and ../code_type = 'yes'" constraint="regex(.,'^[A-Z]{6}-\d{9}$')" jr:constraintMsg="jr:itext('/data/contact/code_w:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/code_x" type="string" relevant="../meta/created_by_role = &quot;chw&quot;" constraint="regex(.,'^[A-Z]{0}\d{4}$')" jr:constraintMsg="jr:itext('/data/contact/code_x:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/code_y" type="string" relevant="../meta/created_by_role !=&quot;chw&quot; and ../code_type != 'yes'" constraint="regex(.,'^[A-Z]{6}-\d{3}$')" jr:constraintMsg="jr:itext('/data/contact/code_y:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/contact/unique_code" type="string" calculate="coalesce(coalesce(../code_w,../code_x),../code_y)"/>
      <bind nodeset="/data/contact/external_id" type="string" calculate="if(concat(../meta/created_by_role,'')=&quot;chw&quot;,concat(../meta/created_by_external_id,'00',../unique_code),concat(../unique_code,''))"/>
      <bind nodeset="/data/contact/code" type="string" calculate="../external_id"/>
